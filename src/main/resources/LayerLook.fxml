<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<fx:root fx:id="background" onMouseClicked="#selectLayer" prefHeight="36.0" prefWidth="152.0" styleClass="layer-item" stylesheets="@Stylesheet.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="layerName" layoutX="34.0" layoutY="7.0" prefHeight="18.0" prefWidth="106.0" styleClass="layerTextField" text="layerName">
         <font>
            <Font size="10.0" />
         </font>
      </TextField>
      <CheckBox fx:id="layerVisible" layoutX="8.0" layoutY="9.0" mnemonicParsing="false" onAction="#toggleLayerVisible" selected="true" />
   </children>
</fx:root>
